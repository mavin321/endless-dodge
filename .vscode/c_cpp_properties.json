{
  "version": 4,
  "configurations": [
    {
      "name": "WSL",
      "compilerPath": "/usr/bin/gcc",
      "intelliSenseMode": "linux-gcc-x64",
      "cStandard": "c99",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/SDL2"
      ],
      "defines": ["_REENTRANT"],
      "browse": {
        "path": [
          "${workspaceFolder}",
          "/usr/include/SDL2"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}
